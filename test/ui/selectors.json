{
  "title": "h1",
  "phaseTable": {
    "_contents": {
      "weekLabels": ".phase-table > thead .week-label",
      "week": {
        "_selector": ".phase-table .phase-week",
        "_contents": {
          "link": "a"
        }
      }
    }
  },
  "phaseWeekOverview": {
    "_selector": ".week-info",
    "_contents": {
      "weekStart": ".start time"
    }
  },
  "phaseWeek": {
    "_contents": {
      "day": {
        "_selector": ".day",
        "_contents": {
          "front": {
            "_selector": ".front",
            "_contents": {
              "phase": ".utilization-phase",
              "leaveRequestType": ".utilization-leave-request-type"
            }
          },
          "typeInput": ".back select:nth-child(1)",
          "leaveRequestTypeInput": ".back select:nth-child(2)",
          "initiativeInput": ".back select:nth-child(3)",
          "projectInput": ".back select:nth-child(4)",
          "phaseInput": ".back select:nth-child(5)",
          "saving": ".saving",
          "set": ".back .set"
        }
      },
      "nav": {
        "_contents": {
          "prev": ".week-nav .prev",
          "next": ".week-nav .next"
        }
      },
      "dayLabels": "thead .labels th:not(:first-child)",
      "employee":  "tbody .name",
      "weekStart": ".week-info .start time",
      "notes": ".notes textarea",
      "verifyBtns": ".btn-verify",
      "submit": ".submit .btn",
      "verified": ".verification:checked"
    }
  },
  "notifications": {
    "_selector": ".notification-list",
    "_contents": {
      "item": ".ntfn-item",
      "error": ".ntfn-item.error"
    }
  },
  "session": {
    "loginButton": ".login-prompt .login-button",
    "logoutButton": ".app-header .log-out"
  }
}
